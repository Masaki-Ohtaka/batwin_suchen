<%# <%= form_with model: user, url: user_registration_path, local: true do |f| %>
          <%= devise_error_messages! %>
          
          <table border="0" cellpadding="0" cellspacing="0">
            <tbody>
              <tr>
                <th>
                  <%= f.label :email, "メールアドレス" %><%# <label for="form_email">メールアドレス</label>%>
                  <b>※</b>
                </th>
                <td>
                  <%= f.email_field :email, id:"user_email", autofocus: true, autocomplete: "email" %><%# <input style="width:270px;" name="email" value="" type="text" id="form_email">　半角英数字 %>
                  <br>
                  <div style="margin-top:13px;line-height:18px;">
                    ※できるだけパソコンのアドレスでご登録お願いします（写真を送付する関係）<br>
                    ※携帯のアドレスをご使用の場合はドメイン受信拒否設定を解除してください。</div>
                </td>
              </tr>

              <tr>
                <th>
                  <%= f.label :password, "パスワード（6文字以上）" %>
                  <% if @minimum_password_length %>
                  <% end %><b>※</b>
                </th>
                <td>
                  <%= f.password_field :password, id:"user_password", autocomplete: "new-password" %>　半角英数字
                  <br>
                </td>
              </tr>

              <tr>
                <th>
                  <%= f.label :password_confirmation, "パスワード再入力" %> <b>※</b>
                </th>
                <td>
                  <div style="margin-bottom:8px;">確認のため、もう一度ご入力ください。</div>
                  <%= f.password_field :password_confirmation, id:"user_password_confirmation", autocomplete: "new-password" %>　半角英数字
                  <br>
                </td>
              </tr>

              <tr>
                <th>
                  <%= f.label :nickname, "ユーザー名" %>
                  <b>※</b>
                </th>
                <td>
                  <%= f.text_field :nickname, id:"user_name" %>
                  <br>
                </td>
              </tr>

              <tr>
                <th>
                  お名前 
                  <b>※</b>
                </th>
                <td>
                  <table class="form-name" style="width:290px;">
                    <tbody>
                      <tr>
                        <td>
                          <%= f.label :first_name , "姓" %><%# <label style="width:30px;font-weight:normal;" for="form_family_name">姓</label> %>  
                          <%= f.text_field :first_name %><%# <input style="width:95px;" name="family_name" value="" type="text" id="form_family_name">  %>
                          <br>
                        </td>
                        <td>
                          <%= f.label :last_name , "名" %><%# <label style="width:30px;font-weight:normal;" for="form_given_name">名</label> %>
                          <%= f.text_field :last_name %><%# <input style="width:95px;" name="given_name" value="" type="text" id="form_given_name"> %>
                          <br>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>

              <tr>
                <th>
                  お名前（カナ）
                  <b>※</b>
                </th>
                <td>
                  <table class="form-name" style="width:290px;">
                    <tbody>
                      <tr>
                        <td>
                          <%= f.label :first_kana , "セイ" %><%# <label style="width:30px;font-weight:normal;" for="form_family_name_kana">セイ</label> %>
                          <%= f.text_field :first_kana %><%# <input style="width:95px;" name="family_name_kana" value="" type="text" id="form_family_name_kana"> %>
                          <br>
                        </td>
                        <td>
                          <%= f.label :last_kana , "メイ" %><%# <label style="width:30px;font-weight:normal;" for="form_given_name_kana">メイ</label> %>
                          <%= f.text_field :last_kana %><%# <input style="width:95px;" name="given_name_kana" value="" type="text" id="form_given_name_kana"> %>
                          <br>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>

              <tr>
                <th>
                  性別
                  <b>※</b>
                </th>
                
                <td>
                  <div class="mt4">
                    <%= f.label :man , "男性"%><%# <label style="padding:1.5px 20px 0px 7px;vertical-align:top;" for="form_inquiry_ctg_1">お問合せ</label>%>
                    <%= f.radio_button :gender, :man %><%# <input id="form_inquiry_ctg_1" name="inquiry_ctg" value="1" checked="checked" type="radio"> %>
                    <%= f.label :woman ,"女性"%><%# <input id="form_inquiry_ctg_2" name="inquiry_ctg" value="2" type="radio">%>
                    <%= f.radio_button :gender, :woman %><%# <label style="padding:1.5px 20px 0px 7px;vertical-align:top;" for="form_inquiry_ctg_2">見学希望</label> %>
                  </div>
                </td>
              </tr>

              <tr>
                <th>
                  <%= f.label :profile, "職業" %>
                  <b>※</b>
                </th>
                <td>
                  <%= f.text_field :profile %>
                  <br>
                </td>
              </tr>

              <tr>
                <th>生年月日 <b>※</b></th>
                <td>
                  <div class="input-birth-wrap">
                    <%= raw sprintf(
                      f.date_select(
                        :birthday,
                        class:'select-birth',
                        id:"birth-date",
                        use_month_numbers: true,
                        prompt:'--',
                        start_year: 1930,
                        end_year: (Time.now.year - 5),
                        date_separator: '%s'),
                      "<p> 年 </p>", "<p> 月 </p>") + "<p> 日 </p>" %>
                  </div>
                </td>
              </tr>
              <tr>
                <th>ご住所 <b>※</b></th>
                <td>
                  例）171-0022
                  <div style="margin:5px 0px 10px 0px;">
                    <%= f.label :postal_code , "郵便番号" %><%# <input style="width:140px;" name="zipcode" value="" type="text" id="form_zipcode"> %>
                    <%= f.text_field :postal_code %><%# <input type="button" style="margin-left:19px;width:110px;" id="search_address" value="住所を検索">%>
                    <br>
                  </div>
                  
                  <div style="margin:5px 0px 13px 0px;">
                    <%= f.label :location_id, "都道府県" %><%# <input style="width:450px;" name="address" value="" type="text" id="form_address"> %>
                    <%= f.collection_select :location_id, Location.all, :id, :name, {} %>
                    <br>
                  </div>

                  <div style="margin:5px 0px 13px 0px;">
                    <%= f.label :municipality , "市区町村" %>
                    <%= f.text_field :municipality  %>
                    <br>
                  </div>

                  <div style="margin:5px 0px 13px 0px;">
                    <%= f.label :address, "番地" %>
                    <%= f.text_field :address %>
                    <br>
                  </div>

                  <div style="margin:5px 0px 13px 0px;">
                    <%= f.label :building_name , "建物名" %>
                    <%= f.text_field :building_name  %>
                    <br>
                    <div style="margin-top:13px;">※建物名に関しましては空欄でも問題ございません。</div>
                  </div>
                </td>
              </tr>

              <tr>
                <th>
                  <%= f.label :phone_number, "電話番号" %><%# <label for="form_tel">電話番号</label> %> <b>※</b>
                </th>
                <td>
                  <%= f.text_field :phone_number %><%# <input style="width:270px;" name="tel" value="" type="text" id="form_tel">%>　例）03-6809-8950
                  <br>
                  <div style="margin-top:13px;">※お電話番号の明記のない方は、問い合わせの受付を行っておりません。</div>
                </td>
              </tr>
              
            </tbody>
          </table>
        <% if local_assigns[:edit_flag].present? %>
          <div class="actions">
            <%= f.submit "更新", class: :form__btn  %>
        <% else%>
          <div class="actions">
            <%= f.submit "新規登録", class: :form__btn  %>
            
        <% end %>
      </div>